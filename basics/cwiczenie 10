x = int(input("Podaj x: "))
y = int(input("Podaj y: "))
z = str(input("podaj rodzaj operacji: "))

if z=="+":
    print(x+y)
elif z=="-":
    print(x-y)
elif z=="*":
    print(x*y)
elif z=="/" and y!=0:
    print(x/y)
elif z=="/" and y==0:
    print("Dzielenie przez zero!")
else:
    print("Błędny operator")


